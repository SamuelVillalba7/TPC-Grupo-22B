                    <div class="footer__box">
                        <div class="footer__img">
                            <img src="./img/header/logotype.png" alt="" />
                        </div>
                    </div>
                    <div class="footer__box2">
                        <h3 class="footer__title">Quick Links</h3>
                        <ul class="footer__ul">
                            <li class="footer__li"><a href="Default.aspx" class="footer__a">Home</a> </li>
                            <li class="footer__li"><a href="filtros.aspx" class="footer__a">Shop</a> </li>
                            <li class="footer__li"><a href="about.aspx" class="footer__a">About</a> </li>
                            <li class="footer__li"><a href="contact.aspx" class="footer__a">Contact</a> </li>
                        </ul>
